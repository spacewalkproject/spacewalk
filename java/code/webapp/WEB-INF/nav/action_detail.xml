<?xml version="1.0"?>
<rhn-navi-tree label="action_detail" invisible="1" formvar="aid" title-depth="1"
    acl_mixins="com.redhat.rhn.common.security.acl.action.ActionAclHandler">
  <rhn-tab name="Details">
    <rhn-tab-url>/rhn/schedule/ActionDetails.do</rhn-tab-url>
  </rhn-tab>
  <rhn-tab name="Completed Systems">
    <rhn-tab-url>/rhn/schedule/CompletedSystems.do</rhn-tab-url>
  </rhn-tab>
  <rhn-tab name="In Progress Systems">
    <rhn-tab-url>/rhn/schedule/InProgressSystems.do</rhn-tab-url>
  </rhn-tab>
  <rhn-tab name="Failed Systems">
    <rhn-tab-url>/rhn/schedule/FailedSystems.do</rhn-tab-url>
  </rhn-tab>
  <rhn-tab name="Remove Packages" acl="generic_action_type(remove);not action_type_patch()">
    <rhn-tab-url>/rhn/schedule/PackageList.do</rhn-tab-url>
  </rhn-tab>
  <rhn-tab name="Update Packages" acl="generic_action_type(install);not action_type_patch()">
    <rhn-tab-url>/rhn/schedule/PackageList.do</rhn-tab-url>
  </rhn-tab>
  <rhn-tab name="action.nav.backout" acl="generic_action_type(remove);action_type_patch()">
    <rhn-tab-url>/rhn/schedule/PackageList.do</rhn-tab-url>
  </rhn-tab>
  <rhn-tab name="action.nav.install" acl="generic_action_type(install);action_type_patch()">
    <rhn-tab-url>/rhn/schedule/PackageList.do</rhn-tab-url>
  </rhn-tab>
</rhn-navi-tree>
