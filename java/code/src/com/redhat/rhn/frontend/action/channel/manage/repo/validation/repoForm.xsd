<?xml version="1.0"?>

<schema targetNamespace="http://rhn.redhat.com"
        xmlns="http://www.w3.org/1999/XMLSchema"
        xmlns:rhn="http://rhn.redhat.com">

  <attribute name="label">
    <simpleType baseType="string">
      <minLength value="1" />
      <maxLength value="64" />
    </simpleType>
  </attribute>
  <attribute name="url">
    <simpleType baseType="string">
      <minLength value="1" />
      <maxLength value="2048" />
    </simpleType>
  </attribute>

</schema>
